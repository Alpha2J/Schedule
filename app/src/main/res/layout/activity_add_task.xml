<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="10dp">

    <Button
        android:id="@+id/btn_atp_saveOrUpdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="saveOrUpdate"
        />

    <Button
        android:id="@+id/btn_atp_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="update"
        />

    <Button
        android:id="@+id/btn_atp_add_with_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="addWithId"/>

    <Button
        android:id="@+id/btn_atp_addOrUpdate_with_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="addOrUpdateWithId"/>
</LinearLayout>
